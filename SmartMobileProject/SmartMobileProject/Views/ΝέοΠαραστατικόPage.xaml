        <ToolbarItem Text="Ολοκλήρωση" Command="{Binding Αποθήκευση}" IconImageSource="save_white_24.png" />
    </TabbedPage.ToolbarItems>
    <local:ΠαραστατικόΒασικάΣτοιχείαPage />
    <local:ΓραμμέςΠαραστατικώνΠωλήσεωνListViewPage />
   


</TabbedPage>